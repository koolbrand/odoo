odoo.define('@web/core/color_picker/tabs/color_picker_custom_tab', ['@odoo/owl', '@web/core/l10n/translation', '@web/core/registry', '@web/core/utils/colors', '@web/core/color_picker/custom_color_picker/custom_color_picker'], function (require) {
