    static selector = ".input-group";
    static selectorHas = ":scope > .o_show_password";
    dynamicContent = {
        ".o_show_password": {
            "t-on-click": () => this.showPassword = !this.showPassword,
        },
        "input[type='text'], input[type='password']": {
            "t-att-type": () => this.showPassword ? "text" : "password",
        },
        ".o_show_password > i": {
