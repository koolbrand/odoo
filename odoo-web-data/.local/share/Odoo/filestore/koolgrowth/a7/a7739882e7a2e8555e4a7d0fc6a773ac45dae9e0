let __exports = {};
const { constructTreeFromDomain } = require("@web/core/tree_editor/construct_tree_from_domain");
const { introduceVirtualOperators } = require("@web/core/tree_editor/virtual_operators");

__exports.treeFromDomain = treeFromDomain; function treeFromDomain(domain, options = {}) {
    const tree = constructTreeFromDomain(domain, options.distributeNot);
    return introduceVirtualOperators(tree, options);
}

return __exports;
